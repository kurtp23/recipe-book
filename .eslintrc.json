{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "warn",
        "no-unused-vars": "off",
        "radix": "off",
        "object-shorthand": "warn",
        "consistent-return": "off",
        "no-throw-literal": "off",
        "prefer-const": "warn",
        "no-plusplus": "off"
    }
}
